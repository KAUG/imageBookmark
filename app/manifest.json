{
  "manifest_version": 2,

  "name": "Image Bookmark",
  "description": "This extension bookmarks your favorite images.",
  "version": "0.1",

  "permissions": [
    // http://developer.chrome.com/extensions/storage.html
    "storage"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  }
}
